GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2 $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LDCPWebViewController" "${PODS_ROOT}/Headers/Public/LDKit" "${PODS_ROOT}/Headers/Public/LDWebViewProgress"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/LDCPWebViewController" "$PODS_CONFIGURATION_BUILD_DIR/LDKit" "$PODS_CONFIGURATION_BUILD_DIR/LDWebViewProgress"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LDCPWebViewController" -isystem "${PODS_ROOT}/Headers/Public/LDKit" -isystem "${PODS_ROOT}/Headers/Public/LDWebViewProgress"
OTHER_LDFLAGS = $(inherited) -ObjC -l"LDCPWebViewController" -l"LDKit" -l"LDWebViewProgress" -l"stdc++" -l"xml2" -framework "Foundation" -framework "UIKit"
OTHER_LIBTOOLFLAGS = -lstdc++
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
